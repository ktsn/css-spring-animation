{
  "name": "@css-spring-animation/vue",
  "version": "0.3.0",
  "description": "High performance spring animation with CSS Transition",
  "keywords": [
    "spring",
    "animation",
    "css",
    "transition",
    "vue"
  ],
  "author": "Katashin",
  "license": "MIT",
  "homepage": "https://github.com/ktsn/css-spring-animation",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/css-spring-animation.git"
  },
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/css-spring-animation-vue.umd.cjs",
  "module": "./dist/css-spring-animation-vue.js",
  "types": "./types/vue/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/css-spring-animation-vue.js",
      "require": "./dist/css-spring-animation-vue.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vitest typecheck",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run typecheck --run && pnpm run test --run && pnpm build"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "vue": "^3.3.4"
  }
}
